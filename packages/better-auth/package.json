{
  "name": "@refport/better-auth",
  "version": "0.1.0",
  "description": "Better Auth plugin for Refport referral tracking",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.cjs"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsdown",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "refport": "workspace:^"
  },
  "peerDependencies": {
    "better-auth": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "better-auth": "^1.2.0",
    "tsdown": "^0.12.5",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "refport",
    "better-auth",
    "referral",
    "tracking",
    "plugin"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Refport/refport-js",
    "directory": "packages/better-auth"
  }
}
